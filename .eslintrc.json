{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/no-extraneous-dependencies": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "consistent-return": 0,
        "no-tabs": 0,
        "indent": 0,
        "no-prototype-builtins": 0,
        "no-return-assign": 0,
        "no-undef":0,
        "import/newline-after-import": ["error", { "count": 1 }],
        "import/prefer-default-export": 0,
        "operator-linebreak": 0,
        "max-len": 0,
        "object-curly-newline": 0,
        "func-names": 0
    }
}
